<section class="section">
  <div class="container">
    <div class="columns is-4 is-variable">
      <% if user_signed_in? %>
        <%= render 'profile'%>
      <% else %>
        <%= render 'trends' %>
      <% end %> 
      <%= render 'feed', tweets: @tweets %>
      <%= render 'who-to-follow' %>
    </div>
  </div>
</section>


